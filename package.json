{
  "name": "@tktco/node-actionlint",
  "version": "1.6.0",
  "description": "",
  "main": "./build/index.js",
  "bin": "bin/node-actionlint.js",
  "types": "./build/index.d.ts",
  "files": [
    "build",
    "bin",
    "wasm_exec.js",
    "main.wasm",
    "LICENSE.txt"
  ],
  "engines": {
    "node": ">=20.0.0"
  },
  "scripts": {
    "build": "npm run build:go && npm run build:ts",
    "build:go": "make install && make build",
    "build:ts": "zshy",
    "build:tsc": "tsc",
    "clean": "rm -rf build coverage .devcontainer/devcontainer.env",
    "clean:all": "npm run clean && rm -f main.wasm wasm_exec.js",
    "lint": "npm run lint:tsc && npm run lint:prettier",
    "lint:tsc": "tsc --noEmit",
    "lint:prettier": "prettier --check .",
    "lint:fix": "npm run lint:prettier -- --write",
    "test": "npm run build:ts && vitest run",
    "release": "node ./scripts/release.js"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@babel/code-frame": "^7.27.1",
    "chalk": "^5.4.1",
    "fast-glob": "^3.3.3"
  },
  "devDependencies": {
    "@types/babel__code-frame": "^7.0.6",
    "@vitest/coverage-v8": "^1.6.0",
    "execa": "^9.6.0",
    "minimist": "^1.2.8",
    "prettier": "^3.5.3",
    "typescript": "^5.8.3",
    "vitest": "^1.6.0",
    "zshy": "^0.0.15"
  },
  "zshy": {
    "entrypoints": [
      "./src/index.ts",
      "./src/cli.ts"
    ],
    "out": "build",
    "exports": {
      ".": "./src/index.ts",
      "./cli": "./src/cli.ts"
    }
  },
  "module": "./build/index.mjs",
  "exports": {
    ".": {
      "types": "./build/index.d.ts",
      "import": "./build/index.mjs",
      "require": "./build/index.js"
    },
    "./cli": {
      "types": "./build/cli.d.ts",
      "import": "./build/cli.mjs",
      "require": "./build/cli.js"
    }
  }
}
